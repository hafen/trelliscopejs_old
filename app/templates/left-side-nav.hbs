
{{#each leftGroup in leftBarInfoArrC}}
<div class="list-group list-group-sidebar">
  <a href="javascript:" class="list-group-item active list-group-item-primary list-group-item-short">
    <span class="list-group-header">{{ leftGroup.mainTitle }}</span>
  </a>
  {{#each leftItem in leftGroup.items}}
    <a href="javascript:" class="list-group-item" {{bind-attr data-divlink=leftItem.divlink}} {{bind-attr id=leftItem.id}}>
      <i {{bind-attr class=leftItem.icon}}></i>
      <span class="list-group-text">{{ leftItem.title }}</span>
    </a>
  {{/each}}
</div>
{{/each}}
