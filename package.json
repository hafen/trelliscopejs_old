{
  "name": "trelliscopejs",
  "version": "0.0.0",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",

    "old_ebuild": "npm run-script bower_install; ember build",
    "old_ebuild_prod": "npm run-script bower_install; ember build --environment production",

    "old_postinstall": "npm run-script install_ember; npm run-script install_watchman; npm run-script install_phantomjs; npm run-script ebuild",

    "old_test": "ember test",
    "old_test_server": "ember test --server",

    "old_install_ember": "which ember || npm install -g ember",
    "old_install_watchman": "which watchman || brew install watchman",
    "old_install_phantomjs": "which phantomjs || npm install -g phantomjs",

    "old_bower_install": "bower install",

    "watch": "watchify -o js/bundle.js -v -d js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js",
    "test": "jest"

  },
  "repository": "",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.2",
    "bower": "^1.3.12"
  }
}
