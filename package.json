{
  "name": "trelliscopejs",
  "version": "0.0.0",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "main": "app/coffee/app.litcoffee",
  "repository": "github.com/schloerke/trelliscopejs",
  "engines": {
    "node": "0.10.38"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "~3.3.1",
    "broccoli": "^0.15.3",
    "broccoli-cjsx": "0.0.2",
    "broccoli-clean-css": "^1.0.0",
    "broccoli-env": "0.0.1",
    "broccoli-fast-browserify": "^0.2.5",
    "broccoli-less": "^2.0.0",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-replace": "^0.2.0",
    "broccoli-static-compiler": "^0.2.1",
    "broccoli-stew": "0.1.6",
    "broccoli-uglify-js": "^0.1.3",
    "coffee-reactify": "^3.0.0",
    "director": "^1.2.8",
    "flux": "^2.0.1",
    "jquery": "^1.11.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^0.12.1",
    "underscore": "~1.8.2"
  },
  "devDpendencies": {
    "broccoli-cli": "^1.0.0",
    "browserify": "^6.2.0",
    "jest-cli": "~0.1.17"
  },
  "browserify": {
    "transform": [
      [
        "coffee-reactify",
        {
          "extension": ".litcoffee"
        }
      ]
    ]
  },
  "scripts": {
    "start": "broccoli serve",
    "bbuild": "rm -r build/; broccoli build build",
    "bfy": "browserify . > browserify_bundle.js",
    "postinstall": "npm run-script bbuild",
    "test": "jest"
  },
  "jest": {
    "rootDir": "./js"
  }
}
