{
  "name": "trelliscopejs",
  "version": "0.0.0",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",

    "ebuild": "npm run-script bower_install; ember build",
    "ebuild_prod": "npm run-script bower_install; ember build --environment production",

    "postinstall": "npm run-script install_ember; npm run-script install_watchman; npm run-script install_phantomjs; npm run-script ebuild",

    "test": "ember test",
    "test_server": "ember test --server",

    "install_ember": "which ember || npm install -g ember",
    "install_watchman": "which watchman || brew install watchman",
    "install_phantomjs": "which phantomjs || npm install -g phantomjs",

    "bower_install": "bower install"
  },
  "repository": "",
  "engines": {
    "node": ">= 0.12.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.2",
    "ember-cli": "0.2.1",
    "ember-cli-app-version": "0.3.3",
    "ember-cli-babel": "^4.0.0",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "0.0.8",
    "ember-cli-htmlbars": "0.7.4",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.9",
    "ember-cli-uglify": "1.0.1",
    "ember-export-application-global": "^1.0.2",

    "ember-data": "1.0.0-beta.16.1",

    "bower": "^1.3.12",
    "ember-cli-coffeescript": "^0.10.0",
    "ember-cli-less": "^1.3.0",
    "ember-cli-qunit": "0.3.9",

    "express": "^4.12.3"
  }
}
