{
  "name": "trelliscopejs",
  "version": "0.0.0",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "main": "app/coffee/app.litcoffee",
  "repository": "github.com/schloerke/trelliscopejs",
  "engines": {
    "node": "0.10.38"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "~3.3.1",
    "broccoli": "^0.15.3",
    "broccoli-cjsx": "0.0.2",
    "broccoli-clean-css": "^1.0.0",
    "broccoli-cli": "^1.0.0",
    "broccoli-env": "0.0.1",
    "broccoli-fast-browserify": "^0.2.5",
    "broccoli-less": "^2.0.0",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-replace": "^0.2.0",
    "broccoli-static-compiler": "^0.2.1",
    "broccoli-stew": "0.1.6",
    "broccoli-uglify-js": "^0.1.3",
    "browserify": "^6.2.0",
    "coffee-reactify": "^3.0.0",
    "flux": "^2.0.1",
    "jest-cli": "~0.1.17",
    "object-assign": "^1.0.0",
    "react": "^0.13.1",
    "react-ace": "^1.1.5",
    "react-router": "^0.13.2",
    "underscore": "~1.8.2"
  },
  "browserify": {
    "transform": [
      [
        "coffee-reactify",
        {
          "extension": ".litcoffee"
        }
      ]
    ]
  },
  "scripts": {
    "start": "node_modules/.bin/broccoli serve",
    "build_dev": "rm -r build_dev/; node_modules/.bin/broccoli build build_dev",
    "build_prod": "rm -r build_prod/; BROCCOLI_ENV=production node_modules/.bin/broccoli build build_prod",
    "bfy": "node_modules/.bin/browserify . > browserify_bundle.js",
    "postinstall": "npm run-script build_dev; npm run-script build_prod",
    "test": "jest"
  },
  "jest": {
    "rootDir": "./js"
  }
}
